{
  "name": "deep-state",
  "version": "0.0.0",
  "private": true,
  "prettier": "prettier-config-custom",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"*.{js,json}\" && turbo run format",
    "format:check": "prettier --check \"*.{js,json}\" && turbo run format:check",
    "test": "turbo test -- --run",
    "typecheck": "turbo typecheck",
    "publish-packages": "CI=1 turbo run build format:check typecheck test && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2.8.3",
    "prettier-config-custom": "workspace:*",
    "turbo": "^1.7.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.4"
}
